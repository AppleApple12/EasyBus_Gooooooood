<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/view_background"
        android:layout_width="300dp"
        android:layout_height="450dp"
        android:background="@drawable/page601_txv2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_line"
        android:layout_width="6dp"
        android:layout_height="400dp"
        android:background="#d0d0d0"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintStart_toStartOf="@id/view_background"
        app:layout_constraintBottom_toBottomOf="@id/view_bottom"
        android:layout_marginLeft="35dp" />

    <View
        android:id="@+id/view2"
        android:layout_width="280dp"
        android:layout_height="45dp"
        android:background="@drawable/txv_round"
        app:layout_constraintTop_toTopOf="@id/view_background"
        app:layout_constraintStart_toStartOf="@+id/view_background"
        app:layout_constraintEnd_toEndOf="@+id/view_background"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txv_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="   路線名稱：上班"
        android:textSize="20dp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"/>

    <View
        android:id="@+id/view_item1"
        android:layout_width="230dp"
        android:layout_height="30dp"
        android:background="@drawable/txv_round"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@id/view_background"
        app:layout_constraintEnd_toEndOf="@id/view_background"
        android:layout_marginTop="23dp"/>

    <TextView
        android:id="@+id/txv_item1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="直走至靜宜大學(專用道)"
        android:textColor="#000000"
        app:layout_constraintTop_toTopOf="@+id/view_item1"
        app:layout_constraintBottom_toBottomOf="@+id/view_item1"
        app:layout_constraintStart_toStartOf="@+id/view_item1"
        android:layout_marginLeft="40dp"/>

    <View
        android:id="@+id/view_dotbg1"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:background="@drawable/dot_white"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@id/view_background"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/ic_baseline_directions_walk_24"
        android:background="@drawable/dot_greay"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@id/view_background"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="18dp"
        android:padding="5dp"/>

    <View
        android:id="@+id/view_item2"
        android:layout_width="230dp"
        android:layout_height="30dp"
        android:background="@drawable/txv_round"
        app:layout_constraintTop_toBottomOf="@+id/view_item1"
        app:layout_constraintStart_toStartOf="@id/view_background"
        app:layout_constraintEnd_toEndOf="@id/view_background"
        android:layout_marginTop="28.5dp"/>

    <TextView
        android:id="@+id/txv_item2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="直走至靜宜大學"
        android:textColor="#000000"
        app:layout_constraintTop_toTopOf="@+id/view_item2"
        app:layout_constraintBottom_toBottomOf="@+id/view_item2"
        app:layout_constraintStart_toStartOf="@+id/view_item2"
        android:layout_marginLeft="40dp"/>

    <View
        android:id="@+id/view_dotbg2"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:background="@drawable/dot_white"
        app:layout_constraintTop_toBottomOf="@+id/view_dotbg1"
        app:layout_constraintStart_toStartOf="@id/view_background"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="12dp"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/ic_baseline_directions_walk_24"
        android:background="@drawable/dot_greay"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="@id/view_background"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="18dp"
        android:padding="5dp"/>

    <View
        android:id="@+id/view_item3"
        android:layout_width="230dp"
        android:layout_height="30dp"
        android:background="@drawable/txv_round"
        app:layout_constraintTop_toBottomOf="@+id/view_item2"
        app:layout_constraintStart_toStartOf="@id/view_background"
        app:layout_constraintEnd_toEndOf="@id/view_background"
        android:layout_marginTop="28.5dp"/>

    <TextView
        android:id="@+id/txv_item3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="直走至靜宜大學(專用道)"
        android:textColor="#000000"
        app:layout_constraintTop_toTopOf="@+id/view_item3"
        app:layout_constraintBottom_toBottomOf="@+id/view_item3"
        app:layout_constraintStart_toStartOf="@+id/view_item3"
        android:layout_marginLeft="40dp"/>

    <View
        android:id="@+id/view_dotbg3"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:background="@drawable/dot_white"
        app:layout_constraintTop_toBottomOf="@+id/view_dotbg2"
        app:layout_constraintStart_toStartOf="@id/view_background"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="12dp"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/ic_baseline_directions_walk_24"
        android:background="@drawable/dot_greay"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@id/view_background"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="18dp"
        android:padding="5dp"/>

    <View
        android:id="@+id/view_item4"
        android:layout_width="230dp"
        android:layout_height="30dp"
        android:background="@drawable/txv_round"
        app:layout_constraintTop_toBottomOf="@+id/view_item3"
        app:layout_constraintStart_toStartOf="@id/view_background"
        app:layout_constraintEnd_toEndOf="@id/view_background"
        android:layout_marginTop="28.5dp"/>

    <TextView
        android:id="@+id/txv_item4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="直走至靜宜大學"
        android:textColor="#000000"
        app:layout_constraintTop_toTopOf="@+id/view_item4"
        app:layout_constraintBottom_toBottomOf="@+id/view_item4"
        app:layout_constraintStart_toStartOf="@+id/view_item4"
        android:layout_marginLeft="40dp"/>

    <View
        android:id="@+id/view_dotbg4"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:background="@drawable/dot_white"
        app:layout_constraintTop_toBottomOf="@+id/view_dotbg3"
        app:layout_constraintStart_toStartOf="@id/view_background"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="12dp"/>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="40dp"
